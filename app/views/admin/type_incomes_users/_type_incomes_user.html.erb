<div id="<%= dom_id type_incomes_user %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= type_incomes_user.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= type_incomes_user.user_id %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this type incomes user", [:admin, type_incomes_user], class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this type incomes user", edit_admin_type_incomes_user_path(type_incomes_user), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
